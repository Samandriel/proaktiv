{
  "name": "proaktiv-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:db-migrate": "npx dotenv -e .env.local -- prisma migrate dev",
    "dev:db-seed": "npx dotenv -e .env.local -- prisma db seed -- --environment development",
    "dev:db-reset": "npx dotenv -e .env.local -- prisma migrate reset",
    "dev:db-studio": "npx dotenv -e .env.local -- prisma studio",
    "prod:db": "npx dotenv -e .env.production -- prisma migrate deploy"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.16.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "date-fns": "^2.30.0",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "next": "13.4.7",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "dotenv-cli": "^7.2.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "prisma": "^4.16.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
